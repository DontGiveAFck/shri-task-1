{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "no-param-reassign": [2, { "props": false }],
        "quote-props": "off",
        "no-plusplus": "off"
    },
    "env": {
        "browser": true,
        "node": true
    }
}